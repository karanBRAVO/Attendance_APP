<html lang="en">

  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin-Dashboard</title>
    <link rel="stylesheet" href="./headCss/admin.css" />
  </head>

  <body>
    <div>
      <div>
        <h1>Add Data</h1>
      </div>
      <div>
        <form action="/add-new-user/to-database" method="post">
          <div class="inputContainer">
            <input
              type="text"
              name="addUsername"
              id="addUsername"
              placeholder="Username"
              autocomplete="off"
            />
          </div>
          <div class="inputContainer">
            <input
              type="text"
              name="addUserId"
              id="addUserId"
              placeholder="userId"
              autocomplete="off"
            />
          </div>
          <div class="inputContainer">
            <input type="submit" value="Add" />
          </div>
        </form>
      </div>
    </div>
  </body>

</html>